import java.util.Random ;

class Dice_test {
    public static void main(String[] args) {
        dice_test dice = new dice_test() ;
        Dice_eight dice1 = new Dice_eight() ;
        dice.TossDice() ;
        dice1.TossDice() ;
        System.out.println( dice.GetPoint() ) ;
        System.out.println( dice1.GetPoint() ) ;
    } // main
} // class dice_test // six

class dice_test {

    protected int point ;

    public void TossDice() {
        Random ran = new Random() ;// random generate a num
        point = ran.nextInt(6)+1 ;
    } // TossDice()

    public int GetPoint() {
        return point ;
    } // GetPoint()
} // class dice_test // six

class Dice_eight extends dice_test {
    public void TossDice() {
        Random ran = new Random() ;// random generate a num
        point = ran.nextInt(8)+1 ;
    } // TossDice 
} // dice_eight
